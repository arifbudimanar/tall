{
    "name": "arifbudimanar/tall",
    "type": "project",
    "description": "A TALL Stack boilerplate with minimalist design, ready to boost your web app development journey.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": ">=8.3.0",
        "cjmellor/browser-sessions": "^1.1",
        "laravel/framework": "^11.7",
        "laravel/socialite": "^5.14.0",
        "laravel/tinker": "^2.9",
        "livewire/livewire": "^3.4.12",
        "masmerise/livewire-toaster": "^2.2.1"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.13.5",
        "barryvdh/laravel-ide-helper": "^3.0",
        "fakerphp/faker": "^1.23.1",
        "laravel-lang/common": "^6.2",
        "laravel/pint": "^1.15.3",
        "laravel/sail": "^1.29.1",
        "mockery/mockery": "^1.6.11",
        "nunomaduro/collision": "^8.1.1",
        "phpunit/phpunit": "^10.5.20",
        "spatie/laravel-ignition": "^2.7"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "./vendor/bin/pint"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:models -W",
            "./vendor/bin/pint"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}